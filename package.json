{
  "name": "@huntersofbook/i18n",
  "version": "0.8.1",
  "packageManager": "pnpm@8.0.0",
  "description": "It's basically a tool that takes a template with translation keys and syncs them to the given language. You can directly connect the outputs to the i18n libraries you use.",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/huntersofbook/i18n",
  "repository": {
    "type": "git",
    "url": "https://github.com/huntersofbook/i18n.git"
  },
  "bugs": "https://github.com/huntersofbook/i18n/issues",
  "keywords": [
    "unplugin",
    "i18n",
    "i18n template",
    "vite i18n",
    "nuxt i18n"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*",
    "./nuxt": {
      "require": "./dist/nuxt.js",
      "import": "./dist/nuxt.mjs"
    },
    "./types": {
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm build && bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.5",
    "@nuxt/kit": "^3.6.2",
    "chokidar": "^4.0.1",
    "consola": "^3.2.3",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.0",
    "filter-anything": "^4.0.2",
    "globby": "^14.0.2",
    "is-what": "^5.0.2",
    "local-pkg": "^0.4.3",
    "merge-anything": "^6.0.2",
    "minimatch": "^10.0.1",
    "pathe": "^1.1.1",
    "remove-anything": "^2.0.2",
    "schob": "^1.0.2",
    "unplugin": "^1.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@types/debug": "^4.1.8",
    "@types/lodash": "^4.14.195",
    "@types/minimatch": "^5.1.2",
    "@vitest/coverage-c8": "^0.33.0",
    "bumpp": "^9.1.1",
    "eslint": "9.13.0",
    "tsup": "^8.3.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vitest": "^2.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "vue": "^3.3.4"
  }
}
